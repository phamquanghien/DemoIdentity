@using Microsoft.AspNetCore.Identity;
@model IEnumerable<IdentityRole>
<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(m => m.Id)</th>
            <th>@Html.DisplayNameFor(m => m.Name)</th>
            <th>@Html.DisplayNameFor(m => m.NormalizedName)</th>
            <th>@Html.DisplayNameFor(m => m.ConcurrencyStamp)</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th>@Html.DisplayFor(m => item.Id)</th>
                <td>@Html.DisplayFor(m => item.Name)</td>
                <td>@Html.DisplayFor(m => item.NormalizedName)</td>
                <td>@Html.DisplayFor(m => item.ConcurrencyStamp)</td>
                <td><a asp-action="Delete" asp-controller="User" asp-route-id="@item.Id" class="btn btn-success">Delete</a></td>
            </tr>
        }
    </tbody>
</table>